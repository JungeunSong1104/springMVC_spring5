package ch02_pjt_01;

public class MainClass {

	public static void main(String[] args) {
		MyCalculator calculator = new MyCalculator();
		
		/* 클래스를 직접 넘겨주지 않고 연산자를 받자 */
		calculator.calculate(10,5,'+');
		calculator.calculate(10,5,'-');
		calculator.calculate(10,5,'*');
		calculator.calculate(10,5,'/');
		calculator.calculate(10,5,'%');
		
		
		
		
		
		
	}
}
